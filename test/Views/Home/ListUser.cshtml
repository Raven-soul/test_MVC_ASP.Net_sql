<div class="row-2">
    <div class="col db-container">
        <ul>
            @{
                foreach (var user in Model.allUsers) {
                    <li>
                        <a href="https://localhost:44336/Home/EditUser/@user.id" class="db-item" book-id="@user.id">
                            <div class="col db-item-data">
                                <span>@user.id  @user.userName</span>
                            </div>
                        </a>
                    </li>
                }
            }
        </ul>
    </div>
    <div class="col">
        <div class="row buttons-row">
            <div class="col-auto">
                <a href="https://localhost:44336/Form/AddUser/" class="submit-button a-button">
                    Добавить пользователя
                </a>
            </div>
        </div>
    </div>
</div>

